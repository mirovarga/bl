<h1>{{title}}</h1>

<table>
  <tbody>
    {{#posts}}
      <tr>
        <td>{{created}}</td>
        <td><a href="/{{slug}}.html">{{title}}</a></td>
        <td>
          {{#tags}}
            <a href="/tags/{{.}}.html">{{.}}</a>
          {{/tags}}
        </td>
      </tr>
    {{/posts}}
  </tbody>
</table>
